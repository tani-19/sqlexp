create table class;

it should have 3 rows 
ID  NAME
1  abhi
2  adam
4  alex

commit;

INSERT into class
values(5,'Rahul'); commit;
UPDATE class set name='abhijit' where
id='5'; savepoint A;
INSERT into class
values(6,'Chris'); savepoint B;
INSERT into class
values(7,'Bravo'); savepoint C;
SELECT * from class;

rollback to B;
SELECT * from class;

rollback to A;
SELECT * from class;

savepoint B;
